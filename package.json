{
  "name": "rccalendar",
  "private": true,
  "pnpm": {
    "packageExtensions": {
      "busboy": {
        "dependencies": {
          "streamsearch": "1.1.0"
        }
      }
    },
    "overrides": {
      "streamsearch": "1.1.0"
    }
  },
  "scripts": {
    "dev": "pnpm -r run dev --parallel",
    "dev:api": "pnpm --filter api run dev",
    "dev:web": "pnpm --filter web run dev",
    "build": "pnpm -r run build",
    "build:api": "pnpm --filter api run build",
    "build:web": "pnpm --filter web run build",
    "db:generate": "pnpm --filter api run prisma:generate",
    "db:migrate": "pnpm --filter api run prisma:migrate",
    "db:migrate:deploy": "pnpm --filter api run prisma:migrate:deploy",
    "db:seed": "pnpm --filter api run prisma:seed",
    "db:studio": "pnpm --filter api run prisma:studio",
    "smoke:api": "pnpm --filter api run smoke",
    "cleanup:rrule-exdate": "pnpm --filter api run cleanup:rrule-exdate"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.14.2"
}
